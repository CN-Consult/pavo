{
  "name": "pavo",
  "version": "1.0.0",
  "description": "An electron based dashboard app that allows to show multiple websites on a big screen tv easily",
  "homepage": "https://github.com/CN-Consult/pavo",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "generate-docs": "node_modules/.bin/jsdoc -c jsdoc.json"
  },
  "build": {
    "appId": "pavo",
    "files": [
      "**/*",
      "!**/config/*"
    ],
    "linux": {
      "target": "AppImage",
      "category": "Utility"
    }
  },
  "author": {
    "name": "Yannick Lapp",
    "email": "yannick.lapp@cn-consult.eu"
  },
  "license": "MIT",
  "devDependencies": {
    "docdash": "^1.0.2",
    "electron": "^4.0.5",
    "electron-builder": "^20.38.5",
    "jsdoc": "^3.5.5",
    "node": "^12.22.12"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.7.1",
    "@json-editor/json-editor": "^1.3.3",
    "bootstrap": "^4.3.0",
    "bufferutil": "^4.0.7",
    "express": "^4.16.4",
    "express-minify-html": "^0.12.0",
    "glob": "^7.1.3",
    "jquery": "^3.3.1",
    "jquery-ui-dist": "^1.12.1",
    "log4js": "^3.0.6",
    "mv": "^2.1.1",
    "native-toast": "^2.0.0",
    "nunjucks": "^3.1.7",
    "object-equals": "^0.3.0",
    "polyfill-library": "^3.31.1",
    "popper.js": "^1.14.7",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "textfit": "^2.3.1",
    "utf-8-validate": "^5.0.10"
  }
}
